{
  "presets": [
    ["env", { "modules": false }],
    "es2015",
    "stage-2"
  ],
  "ignore":[
    "jquery.js",
    "jquery.min.js",
    "angular.js",
    "angular.min.js",
    "bootstrap.js",
    "bootstrap.min.js"
  ],
  "plugins": ["transform-runtime","transform-vue-jsx"],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
